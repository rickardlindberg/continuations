let main = \() ->
    even? 5 \(bool) -> bool
        (\() -> printNumber 0 exit)
        (\() -> printNumber 1 exit)

let even? = \(n, k) -> isZero? n
    (\() -> k True)
    (\() -> dec n \(res) -> odd? res k)

let odd? = \(n, k) -> isZero? n
    (\() -> k False)
    (\() -> dec n \(res) -> even? res k)

let True  = \(f1, f2) -> f1

let False = \(f1, f2) -> f2

let dec = \(n, k) -> minus n 1 k
